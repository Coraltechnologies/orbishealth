<script>
    function getAge(dateString) {
        var mdate = dateString.toString();
        var yearThen = parseInt(mdate.substring(0, 4), 10);
        var monthThen = parseInt(mdate.substring(5, 7), 10);
        var dayThen = parseInt(mdate.substring(8, 10), 10);
        var today = new Date();
        var birthday = new Date(yearThen, monthThen - 1, dayThen);
        var differenceInMilisecond = today.valueOf() - birthday.valueOf();
        var year_age = Math.floor(differenceInMilisecond / 31536000000);
        var day_age = Math.floor((differenceInMilisecond % 31536000000) / 86400000);
        if ((today.getMonth() == birthday.getMonth()) && (today.getDate() == birthday.getDate())) {
            alert("Happy B'day!");
        }
        var month_age = Math.floor(day_age / 30);
        day_age = day_age % 30;
        if (isNaN(year_age) || isNaN(month_age) || isNaN(day_age)) {
            alert("Invalid birthday - Please try again!");
            document.getElementById("single_cal3").value = '';
            return false;
        } else if (year_age < 0 || month_age < 0 || day_age < 0) {
            alert("Invalid birthday - Please try again!");
            document.getElementById("single_cal3").value = '';
            return false;
        } else {
            var yr_str = year_age + ' Years' //+ month_age + 'M-' + day_age + 'D';
            return yr_str;
        }
    }
    function ageCount() {
        document.getElementById('ageId').value = "";
        var birth_day = document.getElementById("single_cal3").value;
        var date_arr = birth_day.split('/');
        var b = date_arr[0];
        date_arr[0] = date_arr[2];
        date_arr[2] = b;
        dob = date_arr[0] + '-' + date_arr[1] + '-' + date_arr[2];
        var ret_val = getAge(dob);
        if (ret_val) {
            document.getElementById('ageId').value = ret_val;
        }
        //if (year_age < 16) {
        //    $("#minor16tab").show();
        //} else {
        //    $("#minor16tab").hide();
        //}
    }
    function ageCount1() {
        document.getElementById('txtFamilyAge').value = "";
        var date1 = new Date();
        var dob = document.getElementById("single_cal4").value;
        var date2 = new Date(dob);
        var y1 = date1.getFullYear();
        //getting current year
        var y2 = date2.getFullYear();
        //getting dob year
        var age = y1 - y2;
        //calculating age
        if (isNaN(age)) {
            alert("Invalid birthday - Please try again!");
            document.getElementById("single_cal4").value = '';
        } else {
            document.getElementById("txtFamilyAge").value = age + " Years";
