                    for (var i = 0; i < states.length; i++) {
                        var opt = new Option(states[i].Text, i);
                        $("#dropdownservices").append(opt);
                    }
                    $("#dropdownservices").multiselect('rebuild');
                },

            });

            document.getElementById('other_service').value = "";
            return false;
        }
        else {
            alert('Field is empty');
        }
    });
</script>

